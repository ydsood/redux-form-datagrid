{
	"presets": [
		[
			"env",
			{
				"modules": false,
				"targets": {
					"browsers": [
						"Chrome >= 52",
						"FireFox >= 44",
						"Safari >= 7",
						"Explorer 11",
						"last 4 Edge versions"
					]
				},
				"useBuiltIns": true
			}
		],
		"es2015",
		"react",
		"flow"
	],
	"plugins": [
		"syntax-object-rest-spread"
	]
}